<header class="page-header">
  <h1>Existing User Login</h1>
</header>

<%= form_tag '/login' do %>

  Email: <%= text_field_tag :email, nil, class: 'form-field' %>
  Password: <%= password_field_tag :password, nil, class: 'form-field' %>

  <p>
  <%= submit_tag "Submit", class: 'btn btn-lg btn-success' %>
  </p>

<% end %>